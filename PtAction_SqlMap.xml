<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="pub">

  <resultMap id="pt_action_Map" class="org.hnslicom.mp.weixin.pojo.pub.model.tp.PtActionBean" >
	  <result property="actionId" column="ACTION_ID" nullValue="" />
	  <result property="actionName" column="ACTION_NAME" nullValue="" />
	  <result property="actionDesc" column="ACTION_DESC" nullValue="" />
  </resultMap>
  	
  <resultMap id="pt_action_MapWithTranslation" class="org.hnslicom.mp.weixin.pojo.pub.model.tp.PtActionBean" >
  	<result property="actionId" column="action_id" nullValue="" /> 
  	<result property="actionName" column="action_name" nullValue="" /> 
  	<result property="actionDesc" column="action_desc" nullValue="" /> 
  </resultMap>

  <select id="pt_action_selectWithTranslation" resultMap="pt_action_MapWithTranslation" > 
   SELECT 
   <isPropertyAvailable property="extraHint"><isNotEmpty property="extraHint"> 
    /*+ $extraHint$ */
   </isNotEmpty></isPropertyAvailable>
      action_id ,
      action_name ,
      action_desc
   FROM pt_action A
   <dynamic prepend="WHERE">    
      <isPropertyAvailable property="actionId" ><isNotEmpty  prepend="AND" property="actionId"> 
        ( action_id = #actionId:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionIdLikeRange" ><isNotEmpty  prepend="AND" property="actionIdLikeRange"> 
        ( action_id LIKE #actionIdLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionName" ><isNotEmpty  prepend="AND" property="actionName"> 
        ( action_name = #actionName:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionNameLikeRange" ><isNotEmpty  prepend="AND" property="actionNameLikeRange"> 
        ( action_name LIKE #actionNameLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionDesc" ><isNotEmpty  prepend="AND" property="actionDesc"> 
        ( action_desc = #actionDesc:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionDescLikeRange" ><isNotEmpty  prepend="AND" property="actionDescLikeRange"> 
        ( action_desc LIKE #actionDescLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
    <isPropertyAvailable property="rownum" ><isGreaterThan compareValue="0"  prepend="AND" property="rownum"> 
       <![CDATA[  ( ROWNUM <= #rownum#)     ]]> 
    </isGreaterThan></isPropertyAvailable> 
    <isPropertyAvailable property="extraCondition" ><isNotEmpty  prepend="AND" property="extraCondition"> 
        ( $extraCondition$ )
     </isNotEmpty></isPropertyAvailable> 
   </dynamic> 
   <isPropertyAvailable property="extraOrderColumns" ><isNotEmpty property="extraOrderColumns"> 
        ORDER BY $extraOrderColumns$
   </isNotEmpty></isPropertyAvailable>
   <isPropertyAvailable property="forUpdateFlag" >
    <isEqual compareValue="1"  property="forUpdateFlag">
       FOR UPDATE NOWAIT
    </isEqual>
   </isPropertyAvailable>    
  </select>  
  


  <select id="pt_action_select" resultMap="pt_action_Map" > 
   SELECT 
   <isPropertyAvailable property="extraHint"><isNotEmpty property="extraHint"> 
    /*+ $extraHint$ */
   </isNotEmpty></isPropertyAvailable>
   *
   FROM pt_action A
   <dynamic prepend="WHERE"> 
      <isPropertyAvailable property="actionId" ><isNotEmpty  prepend="AND" property="actionId"> 
        ( action_id = #actionId:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionIdLikeRange" ><isNotEmpty  prepend="AND" property="actionIdLikeRange"> 
        ( action_id LIKE #actionIdLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionName" ><isNotEmpty  prepend="AND" property="actionName"> 
        ( action_name = #actionName:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionNameLikeRange" ><isNotEmpty  prepend="AND" property="actionNameLikeRange"> 
        ( action_name LIKE #actionNameLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionDesc" ><isNotEmpty  prepend="AND" property="actionDesc"> 
        ( action_desc = #actionDesc:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionDescLikeRange" ><isNotEmpty  prepend="AND" property="actionDescLikeRange"> 
        ( action_desc LIKE #actionDescLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
    <isPropertyAvailable property="rownum" ><isGreaterThan compareValue="0"  prepend="AND" property="rownum"> 
       <![CDATA[  ( ROWNUM <= #rownum#)     ]]> 
    </isGreaterThan></isPropertyAvailable> 
    <isPropertyAvailable property="extraCondition" ><isNotEmpty  prepend="AND" property="extraCondition"> 
        ( $extraCondition$ )
     </isNotEmpty></isPropertyAvailable> 
   </dynamic> 
   <isPropertyAvailable property="extraOrderColumns" ><isNotEmpty property="extraOrderColumns"> 
        ORDER BY $extraOrderColumns$
   </isNotEmpty></isPropertyAvailable>
   <isPropertyAvailable property="forUpdateFlag" >
    <isEqual compareValue="1"  property="forUpdateFlag">
       FOR UPDATE NOWAIT
    </isEqual>
   </isPropertyAvailable>    
  </select>  

  <select id="pt_action_selectByKey" resultMap="pt_action_Map" > 
   SELECT * FROM pt_action 
   WHERE action_id=#actionId:VARCHAR#
   <isPropertyAvailable property="forUpdateFlag" >
    <isEqual compareValue="1"  property="forUpdateFlag">
       FOR UPDATE NOWAIT
    </isEqual>
   </isPropertyAvailable>      
  </select>  
  
   <update id="pt_action_update"> 
    UPDATE pt_action 
    <dynamic prepend="SET"> 
      <isPropertyAvailable property="actionName" >
        <isNotNull prepend="," property="actionName" >
         action_name = #actionName:VARCHAR#
        </isNotNull> 
      </isPropertyAvailable>   
      <isPropertyAvailable property="actionDesc" >
        <isNotNull prepend="," property="actionDesc" >
         action_desc = #actionDesc:VARCHAR#
        </isNotNull> 
      </isPropertyAvailable>   
    </dynamic>    
    WHERE action_id=#actionId:VARCHAR#
   </update>  
 
   <delete id="pt_action_delete"> 
    delete from pt_action 
    WHERE action_id=#actionId:VARCHAR#
   </delete>  

   <delete id="pt_action_deleteBy"> 
    delete from pt_action WHERE
   <dynamic prepend=" ">    
      <isPropertyAvailable property="actionId" ><isNotEmpty  prepend="AND" property="actionId"> 
        ( action_id = #actionId:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionIdLikeRange" ><isNotEmpty  prepend="AND" property="actionIdLikeRange"> 
        ( action_id LIKE #actionIdLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionName" ><isNotEmpty  prepend="AND" property="actionName"> 
        ( action_name = #actionName:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionNameLikeRange" ><isNotEmpty  prepend="AND" property="actionNameLikeRange"> 
        ( action_name LIKE #actionNameLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
      <isPropertyAvailable property="actionDesc" ><isNotEmpty  prepend="AND" property="actionDesc"> 
        ( action_desc = #actionDesc:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>
     <isPropertyAvailable property="actionDescLikeRange" ><isNotEmpty  prepend="AND" property="actionDescLikeRange"> 
        ( action_desc LIKE #actionDescLikeRange:VARCHAR#)
     </isNotEmpty></isPropertyAvailable>  
    <isPropertyAvailable property="rownum" ><isGreaterThan compareValue="0"  prepend="AND" property="rownum"> 
       <![CDATA[  ( ROWNUM <= #rownum#)     ]]> 
    </isGreaterThan></isPropertyAvailable> 
    <isPropertyAvailable property="extraCondition" ><isNotEmpty  prepend="AND" property="extraCondition"> 
        ( $extraCondition$ )
     </isNotEmpty></isPropertyAvailable> 
   </dynamic>     
   </delete>  

   <insert id="pt_action_insert">
    INSERT INTO pt_action
    (
      action_id ,
      action_name ,
      action_desc
    ) 
    VALUES
    (
      #actionId:VARCHAR# ,   
      #actionName:VARCHAR# ,   
      #actionDesc:VARCHAR#    
    ) 
   </insert>

</sqlMap>	

